{
  "name": "latex-editor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsgo && vite build",
    "preview": "vite preview",
    "check": "tsgo --noEmit",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint:py": "cd texlive-server && uvx ruff check .",
    "format:py": "cd texlive-server && uvx ruff format .",
    "download-engine": "bash scripts/download-engine.sh"
  },
  "dependencies": {
    "monaco-editor": "^0.52.0",
    "pdfjs-dist": "^4.9.155"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@typescript/native-preview": "^7.0.0-dev.20260213.1",
    "lefthook": "^2.1.1",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
